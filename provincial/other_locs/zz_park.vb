$loc = $curloc
if $args[0] = '' or $args[0] = 'main':
	*clr & cla
	$metka = $args[0]
	gs 'time'
	gs 'stat'
	! -- ДЕЙСТВИЯ -- !
	! точки выхода с парка
	gs $curloc,'finish'
	! стандартные действия в парке
	if hour > func('zz_weather','sunrise') and hour <= func('zz_weather','sunset') and sunWeather >= 0: gs $curloc,'actions'
	! -- ОПИСАНИЕ -- !
	gs 'zz_render','Парк',func('zz_funcs','mk_image_ex','common/park/park',1),'Обширный городской парк. У входа "окультурено", но большая часть парка - полудикая, изрезанная сетью дорожек и тропинок. Ночью здесь может быть опасно.'
	! насильник
	if rand(0,100) <= 1 and (hour >= 22 or hour <= 4) and $npc['0,rapist_month'] ! month: gt $curloc,'rapist'
	! вор/грабитель - чем больше налички - тем больше вероятность
	if rand(0,100) <= money/1000 and $npc['0,theft_month'] ! month:
		if func('zz_weather','is_day') = 1:
			gt $curloc,'theft'
		elseif hour >= func('zz_weather','sunset') - 3:
			gt $curloc,'robber'
		end
	end
	! особенности для парка в городке и сити
	if $control_point = 'gorodok':
		if $npc['38,wedding'] = 1 and month >= 5 and month <= 9 and hour >= 16 and hour <= 18 and rand(1,10) = 1 : gs 'zz_render','','pavlovo/family/incest/mini/14.jpg','Гуляя по парку, вы видите свою сестру с Мишей. Кажется, у них все хорошо.'
		if $npc['38,wedding'] = 1 and (month >= 10 and month <= 11) or (month >=3 and month <= 4) and hour >= 16 and hour <= 18 and rand(1,10) = 1 : gs 'zz_render','','pavlovo/family/incest/mini/15.jpg','Гуляя по парку, вы видите свою сестру с Мишей. Кажется, у них все хорошо.'
		if $npc['38,wedding'] = 1 and month >= 12 and month <= 2 and hour >= 16 and hour <= 18 and rand(1,10) = 1 : gs 'zz_render','','pavlovo/family/incest/mini/16.jpg','Гуляя по парку, вы видите свою сестру с Мишей. Кажется, у них все хорошо.'
		! трио с Катей
		if hour = 17 and minut <= 20 and $npc['14,qwKatya'] = 72 and $npc['14,qwKatya_trio_day'] = daystart and month > 3 and month < 11 and sunWeather >= 0: act 'Идти на трио с Катей': gt 'mey_katya_events','park_trio'
		! здесь может звонить бойфренд
		gs 'zz_phone','boyfriend_call_init'
		!--
		if hour >= 10 and hour <= 18: gs 'zz_render','','','Здесь находится небольшой <a href="exec:gt''cinema''">кинотеатр</a>.'
		gs 'zz_render','','','Кафешка <a href="exec:gt''cafe_parco'',''start''">"Del Parco"</a> из-за своего броского внешнего вида выглядит в парке как не из этого мира.'
		! вход ветки Рекса
		if rexPark = 1 and rexParkDay = daystart and hour >= 16 and hour < 18:
			gs 'zz_render','','','Вы видите, что в парке прогуливается Рекс'
			act 'Подойти к Рексу':gt 'reks','skver'
		end
		! бойфрейнд гуляет в парке
		if GvstreBoy = 1: gs 'zz_render','','','Вы видите, что в парке прогуливается <a href="exec:numnpc = SkverBoy & GT ''Gnpc''"><<$nameBoyfrend[SkverBoy]>></a>.'
		! гопота
		if sunWeather >= 0 and temper > 0 and ((hour >= 18 and hour < 20 and week < 6) or (week >= 6 and hour >= 10 and hour < 20)):
			gs 'zz_render','','','Вы видите как в парке на лавочке сидят <a href="exec: gt''gopskver''">Витёк Котов, Дэн Рыжов и Шульга.</a>'
		end
		! мать гуляет в парке
		gs 'zz_family','mother_sheduler'
		! meynold`s events
		if $npc['15,qw_day'] ! day: gs 'mey_vika_events', 'park_meet'
	else
		! проверка машини возле парка
		gs 'car', 'check'
		! кафешка в парке
		if hour >= 10 and hour <= 20:
			act 'Посидеть в кафе в парке':
				*clr & cla
				minut += 30
				gt 'parkKafe','food'
			end
		end
		! наркоманы
		if hour > func('zz_weather','sunrise') and hour < func('zz_weather','sunset') and pusher_day ! day:
			pusher_day = day
			if narkossister <= 3 and SNarkPriton = 0: gs 'zz_render','','','Возле одной из лавок валяются <a href="exec:gt''zz_park'',''nark''">торчки</a>.'
			if pusher > 0:
				gs 'zz_render','','','Вы видите торговца дурью, трущегося у входа в парк.'
				act 'Купить бледную леди (2000 руб 1 доза)':
					cla
					minut += 10
					i = input ("Сколько доз вы хотите купить?")
					if i*2000 >= money or i <= 0:
						gs 'zz_render','','','<font color=red>У вас недостаточно денег для покупки!</font>'
					else
						money -= i*2000
						dur += i
						gs 'zz_render','','common/park/drug_dealer.jpg','Вы быстро рассчитались и спрятали пакетик, переданный вам торговцем, в карман. Потом вы сможете нюхнуть у себя дома в спальне.'
					end
					act 'Уйти': gt 'zz_park'
				end
				exit
			end
		end
		! если знакомы с Таней - можно c ней поболтать
		if hour >= 18 and hour <= 20 and week < 6 and $npc['31,qwTanyaMain'] >= 10:
			gs 'zz_render','','','В парке бегает <a href="exec:gt''tanya_events'',''talk''">Таня.</a>'
		end
		! бойфренд
		if bfa >= 1 and hour >= 15 and rand(0,10) <= 1: gt $curloc,'boyfriend'
		! дополнительные переходы в парке сити
		act 'Идти к панели возле парка': gt $curloc,'whore'
		act 'Идти к аттракционам': gt 'zz_park','lunapark'
	end
end
! стандартные действия в парке
if $args[0] = 'actions':
	! раздача флаеров
	if hour >= 10 and hour <= 15:
		act 'Подрабатывать раздачей флаеров (100 руб в час)':
			*clr & cla
			minut += 60
			money += 100
			manna -= 2
			gs 'stat'
			gs 'time'
			gs 'zz_render','',func('zz_funcs','mk_image','common/park/flaer'),'Вы целый час раздавали флаеры посетителям парка. После этого вы получили 100 рублей.'
			act 'Закончить': gt $curloc
		end
	end
	! бег в парке
	if func('zz_clothing','get_group') = 3 and running_day ! day and sick < 1:
		act 'Бегать по парку (1 час)':
			*clr & cla
			gs 'zz_common','crossing'
			gs 'time'
			! входная точка ветки с Таней
			if hour >= 18 and hour <= 20 and week < 6 and $npc['31,qwTanyaMain'] < 10 and tanya['park_meet_day'] ! day and dom >= 50 and $control_point ! 'gorodok': gt 'tanya_events','park_meet'
			! --
			! маньячило
			if hour >= func('zz_weather','sunset'):
				if RAND(0,10) >= 8:
					gs 'zz_render','','','Из тени дерева вышел парень, он подошел к вам и процедив сквозь зубы "Шалава!", потащил вас в кусты.'
					act 'Драться': gt 'zz_fight',2
					exit
				end
			end
			! --
			act 'Далее': gt $curloc
		end
	end
	! прогулка в парке
	act 'Гулять по парку (30 мин)':
		*clr & cla
		minut += 30
		manna += 10
		gs 'stat'
		if pusher = 0 and $control_point ! 'gorodok':
			*clr & cla
			gs 'zz_render','Странная личность','common/park/drug_dealer.jpg','Молодой парень, одетый в кофту с капюшоном, подходит к вам и заговорщицким голосом говорит:**\\\- Девушка, у меня тут доза бледной леди есть, хочешь попробовать? Совершенно бесплатно. Прекрасная вещь, моментально прочищает мозги, настроение такое, что просто атас, ощущения в теле такие, как будто вы здоровее никогда не были.///'
			act 'Да':
				cla
				pusher = 1
				manna = 100
				nark += 10
				narkday = day
				gs 'stat'
				gs 'zz_render','','','Вы остановились с интересом, никогда не пробовали дурь, но ведь с одного раза ничего не будет? Парень подошел к вам и отсыпал немного белого порошка, вы затянули его носом и мир расцвел неожиданными красками.<br>Парень посмотрел на вас и сказал:**\\\- Если понравилось, приходи еще, доза всего 2000 рублей.///'
				act 'Уйти': gt 'zz_park'
			end
			act 'Нет': gt 'zz_park'
			exit
		end
		! миники
		if rand(0,10) < 2:
			gt $curloc,'mini'
		else
			if $npc['25,qwSonya'] >= 5 and $npc['25,qwSonya'] < 50 and hour >= 15 and hour < 20 and $control_point = 'gorodok':
				! Соня отбивает ГГ от гопоты
				if $npc['25,qwSonya'] = 15: gt 'sonya','help_in_park'
				! Соня сосет в парке
				if rand(0,10) = 1: gt 'sonya','suck_in_park'
			end
			gs 'zz_render','',func('zz_funcs','mk_image','common/park/walk'),'Вы не торопясь прогуливаетесь по парку, дыша свежим воздухом.'
			act 'Завершить прогулку': gt $curloc
		end
	end
	! посиделки на лавочке
	if temper >= 5:
		act 'Присесть на лавку':
			*clr & cla
			if func('zz_clothing','get_group') = 2:
				_park_img = 0
			elseif func('zz_clothing','is_schooldress') = 1:
				_park_img = 1
			elseif func('zz_clothing','get_group') = 4:
				_park_img = 2
			else
				_park_img = 3
			end
			gs 'zz_render','','common/park/bench/'+_park_img,'Ну и какого хрена она здесь делает?'
			gs 'zz_funcs', 'waiting'
			if reading['in_bag'] >= 0: act 'Читать '+func('zz_books','get_name',reading['in_bag']): gt 'zz_books','read_on_walk'
			! если ГГ бухая - может уснуть на лавке
			if alko >= 3 and rand(0,10-alko) <= 1:
				*clr & cla
				health += 5
				hapri = 0
				i = rand(1,alko/3)
				minut += i*60
				son += i*3
				if mop > 1: mop = 0 & vidageday -= 1
				gs 'stat'
				gs 'zz_render','','common/park/bench/sleep/'+_park_img,'Вы казалось только присели чуть отдохнуть на лавочке - как на вас накатила такая усталость и сморил сон.'
				act 'Потянуться':
					*clr & cla
					if func('zz_clothing','is_skirt') = 1:
						if tanga = 1:
							tanga = 0
							gs 'zz_render','','common/park/bench/sleep/4','Вы сладко потянулись, разведя руки и ноги, и почувствовали, что трусики куда-то делись! Испуганно сжав ножки, вы подумали: «надеюсь, кто забрал трусики - только ими и ограничился, а не воспользовался вашим состоянием и не сделал чего-то большего».'
						else
							gs 'zz_render','','common/park/bench/sleep/5','Вы сладко потянулись, разведя руки и ноги, сверкая своей голой писькой на весь парк.'
						end
					else
						gs 'zz_render', '', 'common/park/bench/sleep/6','Вы сладко потянулись.'
					end
					act 'Встать': gt $curloc
				end
				killvar '_park_img'
				exit
			end
			killvar '_park_img'
			exit
		end
	end
end
! выход с парка
if $args[0] = 'finish':
	if $control_point = 'gorodok':
		act 'Выйти из парка':
			minut += rand(5,10)
			gt 'gorodok'
		end
	else
		act 'В Северный район':
			minut += rand(20,40)
			gt 'nord'
		end
		act 'В Южный район':
			minut += rand(20,40)
			gt 'street'
		end
		act 'В Центр':
			minut += rand(20,40)
			gt 'down'
		end
	end
end
! наркоманы
if $args[0] = 'nark':
	*clr & cla
	gs 'time'
	SNarkPriton = 1
	gs 'zz_render','Упоротые','common/park/druggy.jpg','Торчки посмотрели на вас тупым отрешенным взглядом. Но один из них сказал:**\\\- Слыш, если дурь нужна, иди на хату, тут не продаем.///'
	act 'Отойти': gt 'zz_park'
end
! аттракционы
if $args[0] = 'lunapark':
	*clr & cla
	minut += 5
	gs 'stat'
	gs 'time'
	gs 'zz_render','Аттракционы','common/park/lunapark/lunapark.jpg','Аттракционы занимают достаточно большую часть парка и днём тут всегда суета из отдыхающих, влюбленных парочек и мамаш с детьми.'
	if vladimirQW = 0 and hour >= 12 and hour <= 18 and week > 5 and vnesh >= 40: gs 'zz_render','','','<a href="exec:GT ''qwloc''">Около ограды стоит крепкий мужчина и разглядывает вас</a>.'
	if hour >= 9 and hour <= 20:
		if money >= 75:
			act 'Съесть шаурму 75 руб':
				*clr & cla
				minut += 30
				health += 10
				money -= 75
				manna += 5
				cumlip = 0
				gs 'stat'
				gs 'zz_kitchen_acts','eat','images/common/park/lunapark/shaurma.jpg'
				act 'Отойти': gt 'zz_park','lunapark'
			end
		end
		if money >= 50:
			act 'Попить лимонада 50 руб':
				*clr & cla
				minut += 30
				money -= 50
				manna += 5
				cumlip = 0
				gs 'stat'
				gs 'zz_kitchen_acts','drink','','images/common/park/lunapark/lemonade.jpg'
				act 'Отойти': gt 'zz_park','lunapark'
			end
			act 'Ударить по силомеру (50 руб)':
				*clr & cla
				minut += rand(5,10)
				money -= 50
				gs 'stat'
				gs 'zz_render','','common/park/lunapark/silomer.jpg','Вы, что есть силы ударили по силомеру и на экране зажглась цифра ' + rand(stren*4/5,stren*6/5)
				act 'Отойти': gt 'zz_park','lunapark'
			end
		end
		if money >= 100:
			act 'Кататься на карусели 100 руб':
				*clr & cla
				minut += 30
				money -= 100
				manna += 10
				gs 'stat'
				gs 'zz_render', '', 'common/park/lunapark/karusel.jpg', 'Вы полчаса катались на карусели, и от этого увеличилось настроение'
				act 'Отойти': gt 'zz_park','lunapark'
			end
			act 'Стрелять в тире 100 руб':
				*clr & cla
				minut += 30
				money -= 100
				gs 'stat'
				gs 'zz_render', '', 'common/park/lunapark/tir.jpg', 'Вы решили попробовать себя в роли стрелка в тире. Перед вами несколько мишеней и всего 10 пулек.'
				act 'Стрелять':
					cla
					i = rand(agil - agil/5,agil + agil/5)
					if i >= 80:
						mishka += 1
						manna += 10
						gs 'zz_render','','','Вы точно положили все 10 пуль в цели и выиграли плюшевого мишку.'
					else
						manna += 5
						gs 'zz_render','','','<<i/8>> из 10 пуль попали в цель.'
					end
					gs 'stat'
					act 'Отойти': gt 'zz_park','lunapark'
				end
			end
		end
	end
	act 'Уйти': gt 'zz_park'
end
